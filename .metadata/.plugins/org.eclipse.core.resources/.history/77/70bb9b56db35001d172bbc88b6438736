package conditionals;

public class conditionals {

	public static void main(String[] args) {
		
		int sayi = 20;
		if (sayi<20) {
			System.out.println("Sayı 20 den küçüktür");
		}
		
		if (sayi>15) {
			System.out.println("Sayı 15 ten büyüktür");	
				
		}
		 if (sayi = sayi) {
			 System.out.println("Sayı 20ye eşittir");
		 }
		
		else {
			System.out.println("Sayı 20ye eşittir");
			
			
		}
    
			
	}

}
